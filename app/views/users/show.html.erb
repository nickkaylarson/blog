<% if current_user %>
  <div class="text-center mt-5">
    <table class="table table-light">
      <thead>
      <tr>
        <th class="display-6"><%= t '.name' %></th>
        <th class="display-6"><%= t '.email'%></th>
      </tr>
      </thead>
      <tbody>
      <tr>
        <td><%= current_user.name %></td>
        <td><%= current_user.email %></td>
      </tr>
      </tbody>
    </table><br>

    <%= render partial: 'partials/post', locals: { user: current_user } %>
  </div>
<% else %>
  <р2>Not your page!</р2>
<% end %>
